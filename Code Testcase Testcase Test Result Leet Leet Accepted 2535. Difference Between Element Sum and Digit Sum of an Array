class Solution {
    public int differenceOfSum(int[] nums) {
        int elsum = 0;
        int digsum = 0;
        for(int i=0;i<nums.length;i++){
            elsum+= nums[i];
            while(nums[i]>0){
                int dig = nums[i]%10;
                digsum+= dig;
                nums[i]/=10;
            }
        }
        return elsum-digsum;
    }
}
